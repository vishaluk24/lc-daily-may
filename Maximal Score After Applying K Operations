class Solution {
public:
    long long maxKelements(vector<int>& nums, int k) {
        long long ans=0;
        int n=nums.size();
        priority_queue<int> pq;
        for(int i=0;i<n;i++){
            pq.push(nums[i]);
        }
        while(k>0){
            int element=pq.top();
            pq.pop();
            ans+=element;
            int currval=ceil((double)element/3);
            pq.push(currval);
            k--;
        }
        return ans;
    }
};
